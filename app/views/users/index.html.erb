<p id="notice"><%= notice %></p>

<div class = row>
  <div class="col-md-10 col-md-offset-1">
<h1>Listing Users</h1>
  <table class="table table-condensed">
    <thead>
    <tr>
      <th>Email</th>
      <th>Registered</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
    </thead>
    <tbody>
    <% @users.each do |user| %>
        <tr>
          <td><%= link_to user.email, user %></td>
          <td><%= user.created_at.to_date %></td>
          <td><%= link_to "Edit", edit_user_path(user), :class => 'btn btn-mini' %></td>
          <td><%= link_to("Delete user", user_path(user), :data => { :confirm => "Are you sure?" }, :method => :delete, :class => 'btn btn-mini') unless user == current_user %></td>
        </tr>
    <% end %>
    </tbody>
  </table>
    <%= link_to 'New User', new_user_session_path, class: "btn btn-default"%>
</div>

<br>


</div>
